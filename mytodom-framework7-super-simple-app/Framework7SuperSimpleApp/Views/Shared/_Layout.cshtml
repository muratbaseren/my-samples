<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, minimal-ui, viewport-fit=cover">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="theme-color" content="#2196f3">
    <title>@ViewData["Title"] - Framework7 Super Simple App</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/framework7/5.5.5/css/framework7.bundle.min.css">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css">
    <link rel="stylesheet" href="~/css/main.css">
</head>
<body>
    <div id="app">
        <div class="statusbar"></div>
        @RenderBody()
    </div>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/framework7/5.5.5/js/framework7.bundle.min.js"></script>
    <script type="text/javascript" src="~/js/main.js" asp-append-version="true"></script>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>


    @RenderSection("Scripts", required: false)
</body>
</html>
